eps:=0.0000001;
i_max:=20;
V:=Matrix(Matrix(n,n,shape=identity)):
for iter from 1 to i_max do
count:=0:
for p from 1 to n-1 do
  for q from p+1 to n do

  if (abs(A[p,q])&lt;eps) then next;
  else count:=count+1;
  end if;

  U:=Matrix(Matrix(n,n,shape=identity)):
  div:=A[p,p]-A[q,q];
  if (div&lt;&gt;0) then
    theta:=evalf(1/2*arctan(2*A[p,q]/div));
  else
    theta:=evalf(Pi/4);
  end if;
  s:=sin(theta);
  c:=cos(theta);
  for j from 1 to n do 
    apj:=A[p,j];aqj:=A[q,j];
    A[p,j]:=apj*c+aqj*s;
    A[q,j]:=-apj*s+aqj*c;
  end do;
  for i from 1 to n do 
    aip:=A[i,p];aiq:=A[i,q];
    A[i,p]:=aip*c+aiq*s;
    A[i,q]:=-aip*s+aiq*c;
    vip:=V[i,p];viq:=V[i,q];
    V[i,p]:=vip*c+viq*s;
    V[i,q]:=-vip*s+viq*c;
  end do;

  printf("Iteration %3d:%3d-%3d\n",iter,p,q);
  PrintMatrix(A);
  end do;
end do:
if (count=0) then break; end if;
end do:
</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation>NiM+SSJBRzYiLUknUlRBQkxFR0YlNiUiJ09cKSotSSdNQVRSSVhHRiU2IzcmNyYiIiYiIiUiIiJGMTcmRjBGL0YxRjE3JkYxRjFGMCIiIzcmRjFGMUY0RjBJJ01hdHJpeEc2JEkqcHJvdGVjdGVkR0Y4SShfc3lzbGliR0Yl</Equation></Text-field><Text-field layout="Maple Output" style="2D Output"><Equation>NiM+SSRlcHNHNiIkIiIiISIo</Equation></Text-field></Output><Output><Text-field layout="Maple Output" style="2D Output"><Equation>NiM+SSZpX21heEc2IiIjPw==</Equation></Text-field></Output><Output><Text-field layout="Normal" style="Line Printed Output">Iteration   1:  1-  2
