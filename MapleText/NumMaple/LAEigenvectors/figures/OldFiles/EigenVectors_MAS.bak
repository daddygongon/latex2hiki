V:=Matrix(Matrix(n,n,shape=identity)):
for p from 1 to n-1 do
  for q from p+1 to n do
  U:=Matrix(Matrix(n,n,shape=identity)):
  div:=A[p,p]-A[q,q];
  if (div&lt;&gt;0) then
    theta:=evalf(1/2*arctan(2*A[p,q]/div));
  else
    theta:=evalf(Pi/4);
  end if;
  s:=sin(theta);
  c:=cos(theta);
  U[p,p]:=c;
  U[q,q]:=c;
  U[p,q]:=-s;
  U[q,p]:=s;
  printf("%3d-%3d\n",p,q);
  A:=Transpose(U).A.U:
  V:=V.U;
  PrintMatrix(A);
  end do;
end do:
